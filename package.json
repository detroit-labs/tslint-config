{
  "name": "@detroit-labs/tslint-config",
  "version": "0.0.0-development",
  "description": "Shared TSLint configuration used in Detroit Labs' TypeScript projects",
  "main": "tslint-base.json",
  "scripts": {
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "test": "echo \"No tests yet\"",
    "semantic-release": "semantic-release pre && npm publish --access=public && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/detroit-labs/tslint-config.git"
  },
  "keywords": [
    "tslint",
    "detroit-labs"
  ],
  "author": "Macklin Underdown <macklinu@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/detroit-labs/tslint-config/issues"
  },
  "homepage": "https://github.com/detroit-labs/tslint-config#readme",
  "devDependencies": {
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^2.0.0",
    "husky": "^0.13.3",
    "semantic-release": "^17.0.8",
    "validate-commit-msg": "^2.12.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
